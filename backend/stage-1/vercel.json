{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@now/node",
        "config": { 
            "distDir": "dist" 
        }
      }
    ],
    "routes": [
      {
        "src": "/api/hello",
        "dest": "/src/index.ts"
      }
    ],
    "env": {
        "IPSTACK_API_KEY": "@ipstack_api_key"
      }
  }